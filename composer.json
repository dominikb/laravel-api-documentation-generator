{
  "name": "dominikb/laravel-api-documentation-generator",
  "description": "Simple package that extracts information from Laravel API routes to generate documentation.",
  "keywords": [
    "Laravel",
    "API",
    "Documentation",
    "Generator"
  ],
  "homepage": "https://github.com/dominikb/laravel-api-documentation-generator",
  "license": "MIT",
  "authors": [
    {
      "name": "Dominik Bauernfeind",
      "email": "bauernfeind.dominik@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/config": "5.7.*",
    "illuminate/support": "5.7.*",
    "spatie/regex": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.*",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Dominikb\\LaravelApiDocumentationGenerator\\": "src/",
      "Dominikb\\LaravelApiDocumentationGenerator\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
